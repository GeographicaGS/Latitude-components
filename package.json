{
  "name": "latitud-components",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --target wc --name latitud-component './src/components/**/*.vue'",
    "lint": "vue-cli-service lint",
    "plop": "plop --plopfile plop/plopfile.js",
    "start": "SHADOW_CSS=true vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "storybook:build": "SHADOW_CSS=true vue-cli-service storybook:build -c config/storybook",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^4.1.0 || ^5.0.0",
    "@storybook/addon-knobs": "^4.1.0 || ^5.0.0",
    "@storybook/addon-links": "^4.1.0 || ^5.0.0",
    "@storybook/addon-notes": "^4.1.0 || ^5.0.0",
    "@storybook/vue": "^5.0.5",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "dart-sass": "^1.17.3",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^3.1.1",
    "lint-staged": "^8.1.4",
    "plop": "^2.3.0",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-cli-plugin-styleguidist": "^3.10.1",
    "vue-styleguidist": "^3.10.1",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
